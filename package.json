{
  "name": "rosterserver",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "dev": "set NODE_ENV=development & npm-run-all server",
    "prod": "set NODE_ENV=production & npm-run-all clean build server:prod",
    "server": "babel-node ./src/app.js",
    "server:prod": "node ./dist/bin/www",
    "start": "npm run prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon"
  },
  "dependencies": {
    "apollo-server": "^2.9.6",
    "apollo-server-express": "^2.9.6",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "graphql": "^14.5.8",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "nodemon": "^1.19.1"
  }
}
