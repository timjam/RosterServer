{
  "name": "rosterserver",
  "version": "0.0.1",
  "description": "Server for Roster service",
  "main": "/src/app.js",
  "private": true,
  "scripts": {
    "build": "babel ./src --out-dir dist --copy-files",
    "clean": "rimraf dist",
    "dev": "set NODE_ENV=development & npm-run-all server:dev",
    "lint": "eslint .\\src\\",
    "prod": "set NODE_ENV=production & npm-run-all clean build server:prod",
    "server:dev": "babel-node -r dotenv/config ./src/app.js",
    "server:prod": "node ./dist/bin/www",
    "start": "npm run prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "debug": "~2.6.9",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "pg": "^7.18.1",
    "rimraf": "^3.0.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "nodemon": "^1.19.4"
  }
}
