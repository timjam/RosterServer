{
  "name": "rosterserver",
  "version": "0.1.1",
  "description": "Server for Roster service",
  "main": "/dist/app.js",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "clean": "rimraf dist",
    "dev": "set NODE_ENV=development & npm-run-all server:dev",
    "lint": "npx tslint -p .\\tsconfig.json",
    "prod": "set NODE_ENV=production & npm-run-all clean build server:prod",
    "server:dev": "ts-node --require dotenv/config ./src/app.ts",
    "server:prod": "node ./dist/app.js",
    "start": "npm run prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-session-knex": "^1.6.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "express-session": "^1.17.1",
    "graphql": "^14.5.8",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "^0.20.11",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mssql": "^6.2.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "objection": "^2.1.3",
    "pg": "^7.18.2",
    "rimraf": "^3.0.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/express-promise-router": "^3.0.0",
    "@types/express-session": "^1.17.0",
    "@types/graphql": "^14.5.0",
    "@types/http-errors": "^1.6.3",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/knex": "^0.16.1",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.4",
    "@types/pg": "^7.14.1",
    "@types/rimraf": "^2.0.3",
    "@types/uuid": "^3.4.7",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.3",
    "sqlite3": "^4.1.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2"
  }
}
